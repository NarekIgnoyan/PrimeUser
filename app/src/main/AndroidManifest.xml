<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.narek.primeuserloginregister">
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
        tools:replace="android:icon"
        android:allowBackup="true"
        android:name=".RetrofitJackson.IPC_Application"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainPage.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AfterLogin" />
        <activity android:name=".MainPage.MyCustomDialog" />
        <activity android:name=".PasswordForget" />

    </application>

</manifest>